  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kksharma1618/aws-ssm-parameters-injector/issues"
  },
  "homepage": "https://github.com/kksharma1618/aws-ssm-parameters-injector#readme",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.16",
    "@types/object-path": "^0.9.29",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "ts-node": "^3.0.4",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "object-path": "^0.11.4"
